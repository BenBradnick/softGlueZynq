record(ao, "$(P)clock1desiredFreq") {
	field(DRVH, "100")
	field(DRVL, "10")
	field(PREC, "6")
	field(OUT, "$(P)clock1Seq.DO1 PP")
}
record(seq, "$(P)clock1Seq") {
	field(SDIS, "$(P)clock$(Q)doingInit")
	field(DISV, "1")
	# write desired clock frequency
	field(LNK1, "$(P)clock$(Q)Config.A")
	# select clock
	field(DO2, "1")
	field(LNK2, "$(P)clock$(Q)Config.B")
	# say "write".  asub will clear C field
	field(DO3, "1")
	field(LNK3, "$(P)clock$(Q)Config.C PP")
	# read clock frequency
	field(DOL4, "$(P)clock$(Q)Config.VALA")
	field(LNK4, "$(P)clock1actualFreq PP")
}
record(ao, "$(P)clock1actualFreq") {
	field(PREC, "6")
}

