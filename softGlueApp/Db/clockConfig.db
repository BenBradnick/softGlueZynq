	field(DLY6, ".1")
	field(LNK6, "$(P)clock6desiredFreq.PROC PP")
	field(DLY7, ".1")
	field(LNK7, "$(P)clock$(Q)init.PROC PP")
}
record(bo, "$(P)clock$(Q)init") {
	field(DOL, "1")
	field(OMSL, "closed_loop")
	field(OUT, "$(P)clock$(Q)doingInit PP")
}
record(bo, "$(P)clock$(Q)doingInit") {
	field(OUT, "$(P)clock$(Q)initAll.PROC PP")
}
