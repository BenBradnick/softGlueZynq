record(seq, "$(P)clock$(Q)initWrite") {
	field(PINI, "YES")
	field(DLY1, ".1")
	field(LNK1, "$(P)clock1desiredFreq.PROC PP")
	field(DLY2, ".1")
	field(LNK2, "$(P)clock2desiredFreq.PROC PP")
	field(DLY3, ".1")
	field(LNK3, "$(P)clock3desiredFreq.PROC PP")
